<form method="post" action="{{ path('app_uidcard_delete', {'uid': test}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ test) }}">
    <button class="delete-button">
        <img src="{{ asset('img/deleteIcon.svg') }}" alt="Delete Icon">
    </button>
</form>
